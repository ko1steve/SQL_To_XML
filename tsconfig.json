{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"es2015",
			"es2017",
			"dom"
		],
		"sourceMap": true,
		"noImplicitThis": true,
		"module": "commonjs",
		"allowJs": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"noUnusedLocals": false,
		"baseUrl": "./",
		"plugins": [{ "name": "typescript-plugin-css-modules" }],
    "strictNullChecks": true
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}